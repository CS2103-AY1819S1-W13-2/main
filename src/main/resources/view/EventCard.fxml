<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>

<HBox id="cardPane" fx:id="cardPane" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
    <GridPane HBox.hgrow="ALWAYS">
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
        </columnConstraints>
        <VBox alignment="CENTER_LEFT" minHeight="50" GridPane.hgrow="ALWAYS">
            <padding>
                <Insets bottom="5.0" left="15.0" right="5.0" top="5.0" />
            </padding>
            <HBox alignment="CENTER_LEFT" spacing="5.0">
                <Label fx:id="id" styleClass="cell_small_label" />
                <Label fx:id="time" styleClass="cell_small_label" text="\$time" />
                <Label fx:id="name" styleClass="cell_small_label" text="\$name" />
                <Label fx:id="address" styleClass="cell_small_label" text="\@$address" />
            </HBox>
            <Label fx:id="description" styleClass="cell_small_label" text="\$description">
            <VBox.margin>
               <Insets left="25.0" />
            </VBox.margin></Label>
        </VBox>
      <rowConstraints>
         <RowConstraints />
      </rowConstraints>
    </GridPane>
</HBox>
